 .MensajeExitoso
	background: rgba(black, 0.60);
	position:absolute;
	top:0;
	bottom:0;
	right:0;
	left:0;
	display:flex;
	justify-content:center;
	align-items:center;
	&-ContenidoExitoso
		border-radius:17px;
		background:white;
		padding:1.2em 2em 1.4em;
		.Imagen
			width:7em; 
			height:7em;
			display: inline-block;
			background-image: url(../img/logo1D.png);
			background-size: 100% 100%;
			background-repeat: no-repeat;
		.Texto
			display:inline-block;
			.PExito
				margin-top:2.5em;
				font-size:18px;
				border-bottom: 3px solid rgba(33, 162, 8, 0.88); 
			.BtnAceptar
				background:rgb(232,167,47);
				display:inline-block;
				margin:.7em 2em 0em 5em; 
				padding:.3em 1em; 
			button
				color:black;
				font-size:15px;
				border-radius:7px;
				border: 1px solid rgb(110,110,100);