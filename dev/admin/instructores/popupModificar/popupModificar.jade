div(popup-close).PopupInstructoresModificar
	form.PopupInstructoresModificar-contenidoInstructoresModificar
		figure(style="background-image:url({{instructorDuplicado.imgInstructor}});").ImgModificar
			div.ImgModificar-cambiarImg
				label.labelModificar
					input(type="file" file-upload="instructorDuplicado.imgInstructor").SubirImgModificar
					p cambiar imagen
					i.icomoon.images
		p Nombre:
		input(type="text" name="nombre"  ng-model="instructorDuplicado.nombre" required)
		p Apellido:
		input(type="text" name="apellido" ng-model="instructorDuplicado.apellido" required)
		p.class Clase:
		input(type="text" name="clase" ng-model="instructorDuplicado.clase" required)
		p Correo:
		input(type="text" name="correo" ng-model="instructorDuplicado.correo" required)
		p Celular:
		input(type="text" name="celular" ng-model="instructorDuplicado.celular" required)
		p.peso Peso:
		input.InputPeso(type="text" name="peso" ng-model="instructorDuplicado.peso" required)
		p Estatura:
		input.InputPeso(type="text" name="estatura" ng-model="instructorDuplicado.estatura" required)
		div
			p Edad:
			input.InputEdad(type="text" name="edad" ng-model="instructorDuplicado.edad" required)
		button.BtnCancelar.popup-close Cancelar
		button(ng-click="EditarInstructor()").BtnGuardar Guardar