@import './popupAgregar/popupAgregar.styl'
@import './popupEditar/popupEditar.styl'
@import './popupEliminar/popupEliminar.styl'
//--------------------globales-------------------------	
.NombreAdmin
	color:white;
	font-size:23px;
	margin:1.4em 0em 0em 4em;
	display:inline-block;
.ArticuleAdmin
	width:56em;
	height:25em;
	margin:2em 6em 0em;
	position:relative;
	.UlAdmin
		width:100%;
		height:40em;
		.DivAdmin
			width:100%;
			height:40em;
			.FigureAdmin
				width:100%;
				height:100%;
				position relative;
				background-size cover;
				.PAdmin
					width:40em;
					max-width:45em;
					height:8em;
					margin:12em 0em 0em;
					display:inline-block;
					background-color:rgba(black, 0.60);
					color:white;
					padding:1em 1em 1em .9em;
				.Botones
					float:right;
					background-color:rgba(black, 0.60);
					overflow: hidden;
					border-bottom-left-radius:15px;
					div
						color:white;
						display:inline-block;
						font-size:16px;
						padding:.4em .8em .4em .3em;
						border-right: 1px solid gray;
						margin:.4em .4em;
						cursor:pointer;
						div
						&:last-child
							border:0;
			